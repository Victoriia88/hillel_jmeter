          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="JSR223 PreProcessor">
            <stringProp name="scriptLanguage">groovy</stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">def heroIds = []
for (int i = 1; i &lt;= 100; i++) {
    def heroId = vars.get(&quot;heroId_&quot; + i)
    if (heroId != null) {
        heroIds.add(heroId)
    } else {
        break 
    }
}

log.info(&quot;Hero IDs: &quot; + heroIds.toString())
if (!heroIds.isEmpty()) {
    def randomHeroId = heroIds[new Random().nextInt(heroIds.size())]
    vars.put(&quot;randomHeroId&quot;, randomHeroId)
} else {
    log.error(&quot;No hero IDs found.&quot;)
}
</stringProp>
